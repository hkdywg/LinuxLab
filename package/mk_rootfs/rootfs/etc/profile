
# Ash profile
 
# vim: syntax=sh
 
 
 
# No core files by default
 
ulimit -S -c 0 > /dev/null 2>&1
 
 
 
#USER="`id -un`"

 
LOGNAME=$USER
# /u--username, $PWD :current path
#change username path etc...color is yellow ljx 20210317 
PS1='\[\e[33m\]\u@$HOSTNAME:$PWD$ \[\e[m\]'

 
PATH=$PATH
 
 
 
HOSTNAME=`/bin/hostname`
 
 
 
export USER LOGNAME PS1 PATH






